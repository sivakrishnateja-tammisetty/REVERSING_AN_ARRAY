#include <iostream>
using namespace std;
void p_reverse(int *a,int n)
{
    for(int i=n-1;i>=0;i--)
    {
        cout<<a[i]<<" ";
    }
    cout<<endl;
    //another way
    for(int i=0;i<n;i++)
    {
        cout<<a[n-1-i]<<" ";
    }
}
int main() {
  
  int a[]={1,2,3,4,5};
  int n=sizeof(a)/sizeof(a[0]);
  p_reverse(a,n);
    return 0;
}
